<h2><%= @client.name %></h2>

<ul>
  <strong>Projects:</strong>
  <%= for project <- @client.projects do %>
    <li>
      <%= link(project.name, to: project_path(@conn, :show, project.id)) %>
    </li>
  <% end%>
</ul>

<span><%= link "Edit", to: client_path(@conn, :edit, @client) %></span>
<span><%= link "Back", to: client_path(@conn, :index) %></span>
