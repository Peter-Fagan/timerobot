<%= link {:safe, "&larr; Back to Entries"}, to: entry_path(@conn, :index) %>


<h2>
  <%= @entry.project.name %>
  <%= link "Edit", to: entry_path(@conn, :edit, @entry) %>
</h2>

<ul>
  <li>
    <strong>Project:</strong>
    <%= @entry.project.name %>
  </li>

  <li>
    <strong>Person:</strong>
    <%= @entry.person.name %>
  </li>

  <li>
    <strong>Date:</strong>
    <%= @entry.date %>
  </li>

  <li>
    <strong>Hours:</strong>
    <%= @entry.hours %>
  </li>
</ul>
