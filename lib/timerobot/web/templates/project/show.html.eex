<h2><%= @project.name %></h2>

<ul>
  <strong>Client:</strong>
  <%= @project.client.name %>
  <%= for entry <- @project.entries do %>
    <li>
      Date worked: <%= link(entry.date, to: entry_path(@conn, :show, entry.id)) %> Hours: <%= entry.hours %>
    </li>
  <% end%>
</ul>

<span><%= link "Edit", to: project_path(@conn, :edit, @project) %></span>
<span><%= link "Back", to: project_path(@conn, :index) %></span>
